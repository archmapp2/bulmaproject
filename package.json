{
  "name": "bulma",
  "version": "1.0.0",
  "private": true,
  "description": "git",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js --open",
    "dev": "webpack --config webpack.dev.js && npm run cpB && npm run cpP",
    "watch": "webpack --config webpack.dev.js --watch",
    "build": "webpack --config webpack.prod.js && npm run cpImg && npm run cpB && npm run cpP",
    "cpImg": "copy src\\assets\\img\\*.* docs\\src\\assets\\img",
    "cpB": "md docs\\src\\js 2> nul && copy src\\js\\short*$$.js docs\\src\\js && copy src\\js\\bulma-toast.min.js docs\\src\\js",
    "cpP": "md docs\\src\\sub\\hbs\\partials 2> nul && copy src\\sub\\hbs\\partials\\*.js docs\\src\\sub\\hbs\\partials",
    "cpBB": "npm run cpB",
    "cpX": "xcopy /siYQ .\\src\\sub\\*.* .\\docs\\src\\sub",
    "rmB": "rmdir /S /Q  docs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^4.2.1",
    "file-loader": "^3.0.1",
    "fs-extra": "^9.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.11.2",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "sass": "^1.26.11",
    "sass-convert": "^0.5.2",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.1"
  },
